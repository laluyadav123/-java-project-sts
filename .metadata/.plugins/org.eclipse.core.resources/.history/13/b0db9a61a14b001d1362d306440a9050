import java.util.LinkedHashMap;
import java.util.Map;

public class EmpMain {

	public static void main(String[] args) {
		
		Employee e =new Employee(101, "Lalu", 20000);
		Employee e1 =new Employee(102, "Lalit", 40000);
		Employee e2 =new Employee(103, "Lalu", 30000);
		Employee e3 =new Employee(102, "Aryan", 25000);
		
		
		Map<Employee, String> map =new LinkedHashMap<>();
		map.put(e, "telus");
		map.put(e1, "vipro");
		map.put(e2, "hcl");
		map.put(e3, "congnizent");
		
		System.out.println(map);
		
		
		
	}

}
