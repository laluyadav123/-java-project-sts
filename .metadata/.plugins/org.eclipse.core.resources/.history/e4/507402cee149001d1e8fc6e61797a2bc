package EMployeProject;

public class Employe {
	
	private String name;
	private int salary;
	
	
	public Employe(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}
	
	


	@Override
	public String toString() {
		return "Employe [name=" + name + ", salary=" + salary + "]";
	}
	
	

}
