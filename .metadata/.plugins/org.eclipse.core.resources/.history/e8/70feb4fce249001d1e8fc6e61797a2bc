package EMployeProject;

public class Employe implements Bonus{
	
	private String name;
	int salary;
	
	
	public Employe(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}
	
	public int addbonus() {
		
		this.salary=this.salary+bonus;
		return salary;
		
	}


	@Override
	public String toString() {
		return "Employe [name=" + name + ", salary=" + salary + "]";
	}
	
	

}
